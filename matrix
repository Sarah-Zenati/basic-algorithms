# Produit de matrice 
def matricenulle(n,p):
  s = []
  for i in range(n):
    s.append([])
    for j in range(p):
      s[i].append(0)
  return s


def produitMatrices(A,B):
s = matricenulle(len(A),len(B[0]))
  for i in range(len(A)):
    for j in range(len(B[0])):
      for k in range(len(A[0])):
        s[i][j] += A[i][k]*B[k][j]
  return s
